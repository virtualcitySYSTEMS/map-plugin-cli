{
  "name": "@vcmap/plugin-cli",
  "version": "2.0.11",
  "description": "A CLI to help develop and build plugins for the VC Map",
  "main": "index.js",
  "type": "module",
  "bin": {
    "vcmplugin": "cli.js"
  },
  "scripts": {
    "lint": "eslint . --env node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualcitySYSTEMS/map-plugin-cli.git"
  },
  "author": "Ben Kuster <bkuster@virtualcitysystems.de>",
  "license": "MIT",
  "files": [
    "assets/",
    "src/",
    "cli.js",
    "README.md",
    "index.js",
    "LICENSE.md"
  ],
  "dependencies": {
    "@vcmap/rollup-plugin-vcs-ol": "^1.0.2",
    "@vcsuite/cli-logger": "^1.0.0",
    "archiver": "^5.0.0",
    "commander": "^6.0.0",
    "express": "^4.17.3",
    "prompts": "^2.4.1",
    "sass": "1.32.13",
    "semver": "^7.3.5",
    "vite": "^3.2.0",
    "vite-plugin-vue2": "^2.0.2",
    "vue-template-compiler": "~2.7.3"
  },
  "peerDependencies": {
    "@vcmap/ui": "^5.0.0-rc.15",
    "vue": "~2.7.3"
  },
  "peerDependenciesMeta": {
    "@vcmap/ui": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vcsuite/eslint-config": "^2.1.1",
    "eslint": "^8.9.0"
  },
  "eslintIgnore": [
    "node_modules",
    "assets/helloWorld"
  ],
  "eslintConfig": {
    "extends": "@vcsuite/eslint-config/node",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^@vcmap/ui"
          ]
        }
      ]
    }
  },
  "engines": {
    "node": "^16.14.0",
    "npm": "^8.3.0"
  }
}
